[Unit]
Description=Cart Service

[Service]
User=roboshop
Environment=REDIS_URL="redis://redis-dev.sndp.online:6379"
Environment=CATALOGUE_HOST=catalogue-dev.sndp.online
Environment=CATALOGUE_PORT=8080
ExecStart=/bin/node /app/server.js
SyslogIdentifier=cart
Restart=always

[Install]
WantedBy=multi-user.target
